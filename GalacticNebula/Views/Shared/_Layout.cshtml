<!DOCTYPE html>
<html>
	<head>
		<title>@ViewBag.Title</title>
		<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
		<link href="@Url.Content("~/Content/kendo/kendo.common.min.css")" rel="stylesheet" type="text/css" />
	    <link href="@Url.Content("~/Content/kendojs/kendo.dataviz.min.css")" rel="stylesheet" type="text/css" />
	    <link href="@Url.Content("~/Content/kendojs/kendo.default.min.css")" rel="stylesheet" type="text/css" />
	    <link href="@Url.Content("~/Content/kendojs/kendo.dataviz.default.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/toastr/toastr.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/MsgBox/msgBoxLight.css")" rel="stylesheet" />
        <link href="@Url.Content("~/Content/jquery.fancybox.css")" rel="stylesheet" />

	    <script src="@Url.Content("~/Scripts/kendojs/js/jquery.min.js")"></script>
	    <script src="@Url.Content("~/Scripts/kendojs/js/kendo.all.min.js")"></script>
	    <script src="@Url.Content("~/Scripts/kendo.modernizr.custom.js")"></script>
        <script src="@Url.Content("~/Scripts/toastr/toastr.js")"></script>  
        <script src="@Url.Content("~/Scripts/knockout-2.2.0.js")"></script>
        <script src="@Url.Content("~/Scripts/MsgBox/jquery.msgBox.js")"></script>
        <script src="@Url.Content("~/Scripts/jquery.fancybox.pack.js")"></script>

        


        <style>
            .bg-header {
            background:url(../Images/bg/bg_header.png) repeat-x;
            }
            .bg {
            margin:0 auto; 
       
            }
            #bgRight {
                 width:75%;
                 background-color:#fff;
                 border:solid #e5d9d9 1px; 
                 display:inline-table; 
                 padding:10px 10px 10px 10px; 
                 float:right;
                }
            #bgLeft {
                width:19%; 
                background-color:#fff; 
                border:solid #e5d9d9 1px; 
                display:inline-table; 
                padding:10px 10px 10px 10px;
            }
            #titleMenu {
                font-weight:bold;
                padding:15px 0 0 30px; 
                border:solid #e5d9d9 1px; 
                height:25px; 
                background-image:url(../Images/bg/launch.png);
            }
            #titleRightMenu {
                border:solid #e5d9d9 1px; 
                background-color:#f3f3f3; 
                margin-bottom:10px; 
                padding:10px 0 10px 20px;
            }
            .status {    
            -webkit-border-radius: 2px;
            -moz-border-radius: 2px;
            border-radius: 2px;
            }

        </style>
</head>
	<body>
		<header>
			<div class="content-wrapper">
				<div class="float-left">
					<p class="site-title">@Html.ActionLink("your logo here", "Index", "Home")</p>
				</div>
				<div class="float-right">
					<nav>
						<ul id="menu">
							<li>@Html.ActionLink("Home", "Index", "Home")</li>
							<li>@Html.ActionLink("About", "About", "Home")</li>
							<li>@Html.ActionLink("Contact", "Contact", "Home")</li>
						</ul>
					</nav>
				</div>
			</div>
		</header>
		<div id="body">
			@RenderSection("featured", required: false)
			<section class="content-wrapper main-content clear-fix">
				@RenderBody()
			</section>
		</div>

		<footer>
			<div class="content-wrapper">
				<div class="float-left">
					<p>&copy; @DateTime.Now.Year - My KendoUI ASP.NET MVC Application</p>
				</div>
			</div>
		</footer>
	</body>
</html>
